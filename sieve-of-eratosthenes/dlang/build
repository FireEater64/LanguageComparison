
#Check for existence of dmd before compiling
if hash dmd 2>/dev/null; then
    #Check for existence of dub before compiling
    if hash dub 2>/dev/null; then
        echo "Compiling Dub"
        dub build --config=release --build=release-nobounds
    else
        echo "Error: Dub not found..."
    fi
else
    echo "Error: DMD not found..."
fi
